---
import Layout from "../../layouts/Layout.astro";
import Contact from "../../components/Contact.astro";
import Breadcrumb from "../../components/Breadcrumb.astro";
import Content from "../../components/pages/demarche-eco-conception/Content.astro";
import Hero from "../../components/pages/demarche-eco-conception/Hero.astro";
import Footer from "../../components/Footer.astro";
---

<Layout
  title="Team UX - Démarche d’éco-conception"
  description="Ce site est éco-conçu. Découvrez les détails de notre démarche d’éco-conception dans cette déclaration."
  og_title="Team UX - Démarche d’éco-conception"
  og_description="Ce site est éco-conçu. Découvrez les détails de notre démarche d’éco-conception dans cette déclaration."
>
  <div class="hidden md:block mb-[90px]">
    <Breadcrumb />
  </div>

  <div class="max-md:mt-[60px] mb-[60px]">
    <Hero />
  </div>

  <div class="mb-[200px] lg:mb-[160px]">
    <Content />
  </div>

  <div class="mb-[150px] lg:mb-[170px]">
    <Contact showBackgroundForm={false} />
  </div>

  <Footer />
</Layout>

---
import Picture from "./@common/Picture.astro";
import Yuna from "../images/yuna.png";

const { width, height, class: cssClasses, alt = "", ...props } = Astro.props;
---

<Picture
  src={Yuna}
  alt={alt}
  class:list={["rounded-t-full", "rounded-b-full", cssClasses]}
  loading="eager"
  width={width}
  height={height}
  {...props}
/>

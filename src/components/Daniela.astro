---
import Image from "./@common/Image.astro";

const { width, height, class: cssClasses } = Astro.props;
---

<Image
  srcSet={[
    "/images/daniela.png",
    "/images/daniela.avif",
    "/images/daniela.webp",
  ]}
  alt=""
  width={width}
  height={height}
  class:list={["rounded-t-full", "rounded-b-full", cssClasses]}
/>
